{
  "compilerOptions": {
    "target": "es5",
    "outDir": "./lib",
    "declaration": false,
    "strict": true,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "incremental": true,
    "typeRoots": []
  },
  "types": [],
  "exclude": ["node_modules"],
  "include": ["./src/**/*"]
}

